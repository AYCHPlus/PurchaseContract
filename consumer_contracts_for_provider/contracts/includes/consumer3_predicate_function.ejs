function (request, logger) {
    // TODO: Extract this so that it can be used to test against provider.
    const expectedRequest = {
        "path": "/record",
        "method": "POST",
        "json": {"a": 123, "b": 222, "c": 333},

    }

    if (request.path !== expectedRequest.path) {
        return false;
    }
    if (request.method !== expectedRequest.method) {
        return false;
    }

    try {
        var message = JSON.parse(request.body);
    } catch (e) {
        logger.info("Error in JSON.parse for request.body=" + request.body)
        logger.info(e)
    }
    const assert = require('assert');
    try {
        assert.deepStrictEqual(message, expectedRequest.json);
    } catch (e) {
        logger.info(e)
        return false;
    }

    return true;
}
