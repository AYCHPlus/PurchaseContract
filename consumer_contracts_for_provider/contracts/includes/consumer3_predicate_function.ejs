function (request, logger) {
    const assert = require('assert');
    const fs = require('fs');

    const expectedRequestJson = fs.readFileSync("/home/tdpreece/integration_projects/consumer_driven_contracts_with_mountebank/consumer_contracts_for_provider/contracts/includes/consumer3_expected_request.json");
    const expectedRequest = JSON.parse(expectedRequestJson);

    try {
        assert.equal(request.path, expectedRequest.path);
        assert.equal(request.method, expectedRequest.method);
    } catch (e) {
        logger.info(e);
        return false;
    }

    try {
        var message = JSON.parse(request.body);
    } catch (e) {
        logger.info("Error in JSON.parse for request.body=" + request.body)
        logger.info(e)
    }
    try {
        assert.deepStrictEqual(message, expectedRequest.json);
    } catch (e) {
        logger.info(e)
        return false;
    }

    return true;
}
