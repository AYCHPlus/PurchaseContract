function (request, logger) {
    if (request.path !== "/record") {
        return false;
    }
    if (request.method !== "POST") {
        return false;
    }

    var message = JSON.parse(request.body);
    // TODO: Extract this so that it can be used to test against provider.
    var expectedMessage = {"a": 123, "b": 222, "c": 333};
    const assert = require('assert');
    try {
        assert.deepStrictEqual(message, expectedMessage);
    } catch (e) {
        logger.info(e)
        return false;
    }

    return true;
}
