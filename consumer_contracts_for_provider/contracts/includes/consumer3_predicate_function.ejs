function (request, logger) {
    if (request.path !== "/record") {
        return false;
    }
    if (request.method !== "POST") {
        return false;
    }

    var message = JSON.parse(request.body);
    // TODO: Extract this so that it can be used to test against provider.
    var expected_message = {"a": 123, "b": 222, "c": 333}
    // TODO: This this care about order?
    if (JSON.stringify(message) !==  JSON.stringify(expected_message)) {
        return false;
    }

    return true;
}
