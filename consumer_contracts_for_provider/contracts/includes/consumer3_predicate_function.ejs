function (request, logger) {
    if (request.path !== "/process") {
        return false;
    }
    if (request.method !== "POST") {
        return false;
    }

    var message = JSON.parse(request.body);
    if (!("a" in message)) {
        return false;
    }   
    if (isNaN(message["a"])) {
        return false;
    }
    return true;
}
