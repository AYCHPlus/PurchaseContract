function (request, logger) {
    if (request.path !== "/record") {
        return false;
    }
    if (request.method !== "POST") {
        return false;
    }

    var message = JSON.parse(request.body);
    if (!("a" in message)) {
        return false;
    }   
    if (isNaN(message["a"])) {
        return false;
    }
    if (!("b" in message)) {
        return false;
    }   
    if (isNaN(message["b"])) {
        return false;
    }
    if (!("c" in message)) {
        return false;
    }   
    if (isNaN(message["c"])) {
        return false;
    }
    return true;
}
